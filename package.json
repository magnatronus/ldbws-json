{
  "name": "ldbws-json",
  "version": "1.0.2",
  "engines": {
    "node": ">=7.6"
  },
  "description": "JSON Interface for the National Rail Enquiries Darwin OpenLDBWS SOAP service. Requires a valid token to access services.",
  "author": {
    "name": "SpiralArm Consulting ltd",
    "email": "srogers.uk@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/magnatronus/ldbws-json",
  "repository": {
    "type": "git",
    "url": "https://github.com/magnatronus/ldbws-json"
  },
  "main": "index.js",
  "scripts": {
    "test": "mocha -r ts-node/register ./**/*.spec.ts",
    "test-coverage": "nyc --reporter=text mocha -r ts-node/register ./**/*.spec.ts",
    "dev": "tsc --watch",
    "build": "tsc",
    "build:docs": "jsdoc2md --files ./lib/*.ts --configure ./jsdoc2md.json > ./api.md"
  },
  "keywords": [
    "ldbws",
    "darwin",
    "json"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-typescript": "^7.3.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "chai-interface": "^2.0.3",
    "jsdoc-babel": "^0.5.0",
    "jsdoc-md": "^3.0.0",
    "jsdoc-to-markdown": "^5.0.0",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "lodash": "^4.17.15",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sax": "^1.2.4",
    "xml2js": "^0.4.19"
  }
}
